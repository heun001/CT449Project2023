\n                    <br />\n\n                    <input type=\"submit\" v-bind:value=\"isLoading ? 'Loading...' : 'Register'\" v-bind:disabled=\"isLoading\"\n                        name=\"submit\" class=\"btn btn-primary\" />\n                </form>\n            </div>\n        </div>\n    </div>\n</template>\n \n<script>\nimport axios from \"axios\"\nimport swal from \"sweetalert2\"\nexport default {\n    data() {\n        return {\n            \"isLoading\": false\n        }\n    },\n\n    methods: {\n        doRegister: async function () {\n            const form = event.target;\n            const formData = new FormData(form);\n\n            this.isLoading = true;\n\n            const response = await axios.post(\n                this.$apiURL + \"/registration\",\n                formData\n            );\n\n            this.isLoading = false;\n            swal.fire(\"Registration\", response.data.message, response.data.status)\n\n            if (response.data.status == \"success\") {\n                form.reset();\n            }\n        }\n    }\n}\n</script>"],"mappings":"AAmCA,OAAOA,KAAI,MAAO,OAAM;AACxB,OAAOC,IAAG,MAAO,aAAY;AAC7B,eAAe;EACXC,IAAIA,CAAA,EAAG;IACH,OAAO;MACH,WAAW,EAAE;IACjB;EACJ,CAAC;EAEDC,OAAO,EAAE;IACLC,UAAU,EAAE,eAAAA,CAAA,EAAkB;MAC1B,MAAMC,IAAG,GAAIC,KAAK,CAACC,MAAM;MACzB,MAAMC,QAAO,GAAI,IAAIC,QAAQ,CAACJ,IAAI,CAAC;MAEnC,IAAI,CAACK,SAAQ,GAAI,IAAI;MAErB,MAAMC,QAAO,GAAI,MAAMX,KAAK,CAACY,IAAI,CAC7B,IAAI,CAACC,OAAM,GAAI,eAAe,EAC9BL,QACJ,CAAC;MAED,IAAI,CAACE,SAAQ,GAAI,KAAK;MACtBT,IAAI,CAACa,IAAI,CAAC,cAAc,EAAEH,QAAQ,CAACT,IAAI,CAACa,OAAO,EAAEJ,QAAQ,CAACT,IAAI,CAACc,MAAM;MAErE,IAAIL,QAAQ,CAACT,IAAI,CAACc,MAAK,IAAK,SAAS,EAAE;QACnCX,IAAI,CAACY,KAAK,CAAC,CAAC;MAChB;IACJ;EACJ;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}